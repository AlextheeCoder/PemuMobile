{
  "expo": {
    "name": "Pemu Mobile",
    "scheme": "pemu-mobile",
    "slug": "pemu-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lgo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/lgo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lgo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.max326.pemumobile"
    },
    "web": {
      "favicon": "./assets/images/lgo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37b81c40-4420-46de-820a-ec77312c5328"
      }
    },
    "owner": "pemudeveloper"
  }
}
